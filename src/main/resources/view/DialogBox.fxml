         minWidth="-Infinity" prefWidth="400.0" type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/18">
    <children>
        <Label fx:id="dialog" minHeight="60.0" style="-fx-background-radius: 10px;" stylesheets="@../css/Stage.css"
               text="Label" wrapText="true">
            <HBox.margin>
                <Insets left="20.0" right="20.0"/>
            </HBox.margin>
            <font>
                <Font size="14.0"/>
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </Label>
        <Circle fx:id="imgCircle" fill="#1f93ff" radius="30.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0"/>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0"/>
    </padding>
</fx:root>